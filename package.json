{
  "name": "documentos",
  "version": "1.0.0",
  "description": "A full stack document management system",
  "main": "index.js",
  "scripts": {
    "start": "babel-node server.js",
    "all:tests": "NODE_ENV=test mocha ./server/tests/**/*.spec.js --timeout 120000 --compilers js:babel-register",
    "e2e": "node_modules/.bin/nightwatch --config nightwatch.conf.js",
    "test": "NODE_ENV=test npm run db:test:seed && babel-node ./node_modules/babel-istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- -R spec server/tests/**/*.spec.js --timeout 60000",
    "db:migrate:test:undo": "NODE_ENV=test sequelize db:migrate:undo:all",
    "db:migrate:test": "NODE_ENV=test sequelize db:migrate",
    "db:test:seed": "NODE_ENV=test babel-node ./server/tests/helpers/seedHelpers",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "client:test": "mocha -r mock-local-storage --spec progress ./client/tools/testSetup.js \"./client/src/tests/**/*.spec.js\"",
    "client:watch": "npm run client:test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node bin/buildHtml.js",
    "prebuild": "npm-run-all clean-dist build:html",
    "build": "babel-node bin/build.js",
    "postbuild": "babel-node bin/distServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-rbabalola/documentos.git"
  },
  "keywords": [
    "documents"
  ],
  "author": "Rotimi Babalola",
  "license": "ISC",
  "dependencies": {
    "async-waterfall": "^0.1.5",
    "axios": "^0.15.3",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.23.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.16.1",
    "chai": "^3.5.0",
    "cheerio": "^0.22.0",
    "classnames": "^2.2.5",
    "colors": "^1.1.2",
    "compression": "^1.6.2",
    "dotenv": "^4.0.0",
