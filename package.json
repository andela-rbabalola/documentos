{
  "name": "documentos",
  "version": "1.0.0",
  "description": "A full stack document management system",
  "main": "index.js",
  "scripts": {
    "start": "babel-node server.js --presets es2015,stage-2",
    "test": "NODE_ENV=test npm run db:test:seed && npm run thetest",
    "thetest": "NODE_ENV=test mocha ./server/tests/**/*.spec.js --compilers js:babel-register",
    "db:migrate:test:undo": "NODE_ENV=test sequelize db:migrate:undo:all",
    "db:migrate:test": "NODE_ENV=test sequelize db:migrate",
    "db:test:seed": "babel-node ./server/tests/helpers/seedHelpers"
  },
  "author": "Rotimi Babalola",
  "license": "ISC",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.16.1",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "jsonwebtoken": "^7.3.0",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.30.2",
    "sequelize-cli": "^2.5.1"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "faker": "^4.1.0",
    "logger": "0.0.1",
    "mocha": "^3.2.0",
    "supertest": "^3.0.0"
  }
}
