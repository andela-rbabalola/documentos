{
  "name": "documentos",
  "version": "1.0.0",
  "description": "A full stack document management system",
  "main": "index.js",
  "scripts": {
    "start": "babel-node server.js",
    "test": "NODE_ENV=test npm run db:test:seed && npm run thetest",
    "thetest": "NODE_ENV=test mocha ./server/tests/**/*.spec.js --timeout 20000 --compilers js:babel-register",
    "db:migrate:test:undo": "NODE_ENV=test sequelize db:migrate:undo:all",
    "db:migrate:test": "NODE_ENV=test sequelize db:migrate",
    "db:test:seed": "NODE_ENV=test babel-node ./server/tests/helpers/seedHelpers",
    "cover": "babel-node ./node_modules/babel-istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha server/test/**/*.spec.js",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "author": "Rotimi Babalola",
  "license": "ISC",
